{
	"ACF Register Group": {
		"prefix": "acf_field_group",
		"body": [
			"\\$acf_group = '${1:group_key}';",
			"acf_add_local_field_group(array (",
			"\t'key' => 'group_'.\\$acf_group,",
			"\t'title' => '${2:group_title}',",
			"\t'fields' => array (",
			"\t\t${3:fields}",
			"\t),",
			"\t'location' => array (",
			"\t\tarray (",
			"\t\t\tarray (",
			"\t\t\t\t'param' => 'post_type', // post_type | post | page | page_template | post_category | taxonomy | options_page",
			"\t\t\t\t'operator' => '==',",
			"\t\t\t\t'value' => 'post', // if options_page then use: acf-options  | if page_template then use:  template-example.php or template-folder/template-example.php",
			"\t\t\t\t'order_no' => 0,",
			"\t\t\t\t'group_no' => 1,",
			"\t\t\t),",
			"\t\t),",
			"\t),",
			"\t'menu_order' => 0,",
			"\t'position' => 'normal',  // side | normal | acf_after_title",
			"\t'style' => 'default',  // default | seamless",
			"\t'label_placement' => 'top',  // top | left",
			"\t'instruction_placement' => 'label',  // label | field",
			"\t'hide_on_screen' => array (",
			"\t\t\/\/0 => 'permalink',",
			"\t\t\/\/1 => 'the_content',",
			"\t\t\/\/2 => 'excerpt',",
			"\t\t\/\/3 => 'custom_fields',",
			"\t\t\/\/4 => 'discussion',",
			"\t\t\/\/5 => 'comments',",
			"\t\t\/\/6 => 'revisions',",
			"\t\t\/\/7 => 'slug',",
			"\t\t\/\/8 => 'author',",
			"\t\t\/\/9 => 'format',",
			"\t\t\/\/10 => 'featured_image',",
			"\t\t\/\/11 => 'categories',",
			"\t\t\/\/12 => 'tags',",
			"\t\t\/\/13 => 'send-trackbacks',",
			"\t),",
			"\t'active' => 1,",
			"\t'description' => '',",
			"));"
		],
		"description": "ACF Register Group"
	},
